def soma(x, y):
    """Retorna a soma de dois números."""
    return x + y

def subtract(x, y):
    """Retorna a subtração de dois números."""
    return x - y

def multiply(x, y):
    """Retorna o produto de dois números."""
    return x * y

def division(x, y):
    """Retorna a divisão de dois números, se y for diferente de zero."""
    if y != 0:
        return x / y
    else:
        print("Erro: divisão por zero.")
        return None

#exemplo de contas

resultado_soma = soma(5, 10)
resultado_subtract = subtract(10, 5)
resultado_multiply = multiply(5, 10)
resultado_division = division(5, 5)

print("Resultado da soma:", resultado_soma)
print("Resultado da subtração:", resultado_subtract)
print("Resultado da multiplicação:", resultado_multiply)
print("Resultado da divisão:", resultado_division)
